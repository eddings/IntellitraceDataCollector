        <maml:description>
          <maml:para _locID="parameter.startintellitrace.collectionplan.description">The name and path to the collection plan to use during IntelliTrace collection. The path can be relative or absolute.</maml:para>
        </maml:description>
        <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        <dev:type>
          <maml:name>string</maml:name>
          <maml:uri/>
        </dev:type>
        <dev:defaultValue></dev:defaultValue>
      </command:parameter>
      <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="true (ByValue, ByPropertyName)" position="3">
        <maml:name>OutputPath</maml:name>
        <maml:description>
          <maml:para _locID="parameter.startintellitrace.output.description">The location that IntelliTrace will write the collected data into log files. The identity that is running the application pool must have read/write permissions to the specified location.</maml:para>
        </maml:description>
        <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        <dev:type>
          <maml:name>string</maml:name>
          <maml:uri/>
        </dev:type>
        <dev:defaultValue></dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="true (ByValue, ByPropertyName)" position="named">
        <maml:name>MaximumSize</maml:name>
        <maml:description>
          <maml:para _locID="parameter.startintellitrace.maximumsize.description">
            Specifies the maximum size of the itrace file, in megabytes. This will supersede the value specified in the collection plan. Specify -1 for unlimited size. 
            The size of the itrace must be large enough to hold a minimum amount of data such as metadata.
          </maml:para>
        </maml:description>
        <command:parameterValue required="false" variableLength="false">Int32</command:parameterValue>
