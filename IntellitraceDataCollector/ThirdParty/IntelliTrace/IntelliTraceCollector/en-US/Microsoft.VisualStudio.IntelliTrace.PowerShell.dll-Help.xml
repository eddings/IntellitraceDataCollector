        </dev:type>
        <dev:defaultValue>0</dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="false" position="named">
        <maml:name>Confirm</maml:name>
        <maml:description>
          <maml:para _locID="parameter.startintellitrace.confirm.description">Prompts you for confirmation before executing the command.</maml:para>
        </maml:description>
