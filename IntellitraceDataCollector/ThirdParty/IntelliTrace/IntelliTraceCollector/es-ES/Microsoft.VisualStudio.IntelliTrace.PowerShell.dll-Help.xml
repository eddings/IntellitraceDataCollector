          <maml:description>
            <maml:para _locID="syntax.checkpointintellitrace.applicationpool.description">Nombre del grupo de aplicaciones en el que la recolección de IntelliTrace está habilitada actualmente.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        </command:parameter>
