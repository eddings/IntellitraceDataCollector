        </maml:description>
        <command:parameterValue required="true" variableLength="false">string</command:parameterValue>
        <dev:type>
          <maml:name>string</maml:name>
          <maml:uri/>
        </dev:type>
        <dev:defaultValue></dev:defaultValue>
      </command:parameter>
      <command:parameter required="false" variableLength="false" globbing="false" pipelineInput="true (ByValue, ByPropertyName)" position="named">
        <maml:name>MaximumSize</maml:name>
        <maml:description>
          <maml:para _locID="parameter.startintellitrace.maximumsize.description">
            Gibt die maximale Größe der itrace-Datei an, in Megabytes. Dies ersetzt den im Auflistungsplan angegebenen Wert. Geben Sie -1 für eine unbegrenzte Größe ein. 
            Die Größe von itrace muss groß genug für eine Mindestmenge an Daten sein, z. B. Metadaten.
          </maml:para>
        </maml:description>
        <command:parameterValue required="false" variableLength="false">Int32</command:parameterValue>
