             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" >
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="" Name="tbMessage"  />
            <ProgressBar Name="progressBar" Minimum="0" Width="100" Margin="20,0,0,0" />
            <TextBlock Text="" Name="tbProgress" Margin="3,0,0,0"  />
            <!--<Button Name="btnLocateOM" Content="Locate OM" Visibility="Hidden" Click="btnLocateOM_Click" />-->
        </StackPanel>
    </Grid>
